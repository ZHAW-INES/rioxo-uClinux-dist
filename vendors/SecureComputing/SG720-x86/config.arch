.EXPORT_ALL_VARIABLES:
############################################################################

VENDOR_CFLAGS = -DCONFIG_SNAPGEAR

BUILD_NETtel = 1
LINUXTARGET = bzImage

#Get the version.
include $(ROOTDIR)/vendors/$(CONFIG_VENDOR)/config.version

# To override the vendor-global version do something like:
# VERSIONPKG = 2.3.5u1

VERSIONSTR = $(CONFIG_VENDOR)/$(CONFIG_PRODUCT) Version $(VERSIONPKG)

############################################################################

include $(ROOTDIR)/vendors/config/i386/config.arch

RELFILES := images/bzImage images/configs.tar.gz images/initrd.gz \
			images/SG720-x86-cfg.hdd,-cfg.hdd \
			images/SG720-x86-cfg.hdd.vmdk,-cfg.hdd.vmdk \
			images/SG720-x86-com1.hdd,-com1.hdd \
			images/SG720-x86-com1.hdd.vmdk,-com1.hdd.vmdk \
			images/SG720-x86-com1.iso,-com1.iso \
			images/SG720-x86-hdd.vmx,-hdd.vmx \
			images/SG720-x86-iso.vmx,-iso.vmx \
			images/SG720-x86-vga.hdd,-vga.hdd \
			images/SG720-x86-vga.hdd.vmdk,-vga.hdd.vmdk \
			images/SG720-x86-vga.iso,-vga.iso \

############################################################################
